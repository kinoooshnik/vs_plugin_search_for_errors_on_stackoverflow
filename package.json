{
	"name": "search-for-errors-on-stackoverflow",
	"displayName": "Search for errors on stackoverflow",
	"description": "The VS code extension allows you to quickly find your error on stackoverflow.",
	"version": "0.0.3",
	"publisher": "kinoooshnik",
	"repository": "https://github.com/kinoooshnik/vs_plugin_search_for_errors_on_stackoverflow",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"icon": "img/logo.png",
	"activationEvents": [
		"onCommand:extension.searchProblemOnStackoverflow",
		"onCommand:extension.searchTextOnStackoverflow"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.searchProblemOnStackoverflow",
				"title": "Search problem on stackoverflow"
			},
			{
				"command": "extension.searchTextOnStackoverflow",
				"title": "Search on stackoverflow"
			}
		],
		"keybindings": [
			{
				"command": "extension.searchProblemOnStackoverflow",
				"key": "ctrl+k",
				"mac": "cmd+k"
			},
			{
				"command": "extension.searchTextOnStackoverflow",
				"key": "ctrl+alt+k",
				"mac": "cmd+alt+k"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.searchTextOnStackoverflow",
					"group": "z_commands",
					"when": "editorHasSelection"
				}
			]
		}
	},
	"license": "SEE LICENSE IN LICENSE",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vsce": "^1.69.0",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.8",
		"@types/request-promise-native": "^1.0.8"
	}
}