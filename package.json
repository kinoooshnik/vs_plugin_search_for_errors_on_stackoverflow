{
	"name": "search-for-errors-on-stackoverflow",
	"displayName": "Search for errors on stackoverflow",
	"description": "The VS code extension allows you to quickly find your error on stackoverflow.",
	"version": "0.0.2",
	"publisher": "kinoooshnik",
	"repository": "https://github.com/kinoooshnik/vs_plugin_search_for_errors_on_stackoverflow",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
  "icon": "img/logo.png",
	"activationEvents": [
		"onCommand:extension.searchForErrorOnStackoverflow"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.searchForErrorOnStackoverflow",
				"title": "Search for error on stackoverflow"
			}
		]
	},
	"license": "SEE LICENSE IN LICENSE",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vsce": "^1.69.0"
	}
}
